{
  "name": "maestro",
  "version": "1.1.1",
  "description": "Maestro library for the Firestore backend.",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "start": "npm run shell",
    "test": "nyc ./node_modules/.bin/_mocha 'test/**/*.test.ts' && open coverage/index.html",
    "debugTest": "./node_modules/mocha/bin/mocha --inspect-brk 'test/**/*.test.ts'",
    "publish": "npm run build && npm pack"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage/"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true
  },
  "engines": {
    "node": "14"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@types/uuid": "8.3.0",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.18.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@testdeck/mocha": "0.1.2",
    "@types/chai": "4.2.19",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.8.0",
    "chai": "4.3.4",
    "eslint": "7.6.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-import": "2.22.0",
    "firebase-functions-test": "0.2.0",
    "mocha": "9.0.1",
    "nyc": "15.1.0",
    "ts-node": "10.0.0",
    "typescript": "3.9.10"
  }
}
